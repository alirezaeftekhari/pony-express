{
  "autoload": {
    "psr-4": {
      "PonyExpress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "psr/http-client": "^1.0",
    "ghasedak/php": "^1.0",
    "php-amqplib/php-amqplib": "^3.2"
  },
  "require-dev": {
    "ext-curl": "*",
    "ergebnis/composer-normalize": "^2.0",
    "php-http/client-integration-tests": "*",
    "phpunit/phpunit": "^8.5.5",
    "psr/log": "^1.1"
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
