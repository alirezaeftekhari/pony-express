{
  "name": "pure/pony-express",
  "type": "library",
  "description": "a simple package for sms sending",
  "autoload": {
    "psr-4": {
      "PonyExpress\\": "pony/",
      "App\\": "app/",
      "Core\\": "core/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "php-amqplib/php-amqplib": "^3.2",
    "ghasedak/php": "^1.0",
    "kavenegar/php": "^1.2",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
